# SmokersRights Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to git!

# Admin
ADMIN_PASSWORD=your_admin_password
SMOKERSRIGHTS_ADMIN_PASSWORD=your_admin_password

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# SMS (Sinch)
SINCH_SERVICE_PLAN_ID=your_sinch_service_plan_id
SINCH_API_TOKEN=your_sinch_api_token
SINCH_FROM=+1234567890

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:3010
NEXT_PUBLIC_BASE_URL=http://localhost:3010

# Bot API
BOT_API_KEY=your-secret-bot-key

# API Keys
SMOKERSRIGHTS_API_KEY=your-api-key

# Node Environment
NODE_ENV=development
