        "NEXT_PUBLIC_WEATHERAPI_KEY": { "from": "WEATHERAPI_KEY", "required": false },
        "NEXT_PUBLIC_EDAMAM_APP_ID": { "from": "EDAMAM_APP_ID", "required": false },
