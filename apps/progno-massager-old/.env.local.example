# ============================================
# PROGNO DATA MASSAGER - ENVIRONMENT CONFIG
# ============================================
# Copy this file to .env.local and fill in your values
# AI Safety 2025: All credentials stored locally, never committed

# ============================================
# SUPABASE CONFIGURATION (Memory Engine)
# ============================================
# Get these from: https://supabase.com/dashboard/project/_/settings/api

SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-service-role-key-here

# Alternative names (for compatibility with other apps)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ============================================
# OPTIONAL: RAPIDAPI (For live odds data)
# ============================================
RAPIDAPI_KEY=your-rapidapi-key-here

# ============================================
# OPTIONAL: AI/ML INTEGRATIONS
# ============================================
# For enhanced predictions
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here

# ============================================
# SUPERVISOR AGENT CONFIGURATION
# ============================================
# Set to 'true' to enable autonomous mode (REDUCES SAFETY)
SUPERVISOR_AUTONOMOUS_MODE=false

# Maximum allowed arbitrage profit percentage (sanity check)
MAX_REASONABLE_ARB_PROFIT=15.0

# ============================================
# APPLICATION SETTINGS
# ============================================
# Streamlit server port
PORT=8501

# Debug mode (shows extra validation info)
DEBUG_MODE=false

