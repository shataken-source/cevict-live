{
  "app": "gulfcoastcharters",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "NEXT_PUBLIC_SUPABASE_URL": { "fromAny": ["NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_URL"], "required": true },
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": { "fromAny": ["NEXT_PUBLIC_SUPABASE_ANON_KEY", "SUPABASE_ANON_KEY"], "required": true },
        "SUPABASE_SERVICE_ROLE_KEY": { "fromAny": ["SUPABASE_SERVICE_ROLE_KEY", "SUPABASE_SERVICE_KEY"], "required": true },

        "GCC_ADMIN_EMAILS": { "from": "GCC_ADMIN_EMAILS", "required": false },
        "GCC_ADMIN_KEY": { "from": "GCC_ADMIN_KEY", "required": false },

        "RESEND_API_KEY": { "from": "RESEND_API_KEY", "required": false },
        "RESEND_FROM_EMAIL": { "from": "RESEND_FROM_EMAIL", "required": false },
        "RESEND_REPLY_TO": { "from": "RESEND_REPLY_TO", "required": false },

        "NEXT_PUBLIC_GOOGLE_ADS_CLIENT_ID": { "from": "NEXT_PUBLIC_GOOGLE_ADS_CLIENT_ID", "required": false },
        "NEXT_PUBLIC_AMAZON_AFFILIATE_TAG": { "from": "NEXT_PUBLIC_AMAZON_AFFILIATE_TAG", "required": false }
      }
    }
  ]
}

