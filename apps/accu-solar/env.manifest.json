{
  "app": "accu-solar",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "SUPABASE_URL": { "fromAny": ["SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_URL"], "required": true },
        "NEXT_PUBLIC_SUPABASE_URL": { "from": "NEXT_PUBLIC_SUPABASE_URL", "required": false },
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": { "from": "NEXT_PUBLIC_SUPABASE_ANON_KEY", "required": false },
        "SUPABASE_SERVICE_ROLE_KEY": { "from": "SUPABASE_SERVICE_ROLE_KEY", "required": true },
        "COCHRAN_RUN_MODE": { "from": "COCHRAN_RUN_MODE", "required": false },
        "SUPABASE_PROJECT_REF_SUPABASE_ACCU_SOLAR": { "from": "SUPABASE_PROJECT_REF_SUPABASE_ACCU_SOLAR", "required": false },
        "WEATHERAPI_KEY": { "from": "WEATHERAPI_KEY", "required": false }
      }
    }
  ]
}
