{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "ignoreCommand": "git fetch origin $VERCEL_GIT_PREVIOUS_SHA --depth=1 2>/dev/null; git diff --quiet $VERCEL_GIT_PREVIOUS_SHA $VERCEL_GIT_COMMIT_SHA -- apps/smokersrights/ 2>/dev/null || exit 1",
  "regions": [
    "iad1"
  ],
  "crons": [
    {
      "path": "/api/bot/run",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/fetch-legislation",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/fetch-places",
      "schedule": "0 4 * * 0"
    },
    {
      "path": "/api/cron/fetch-ncsl-cannabis",
      "schedule": "0 3 1 * *"
    }
  ]
}
