{
  "app": "smokersrights",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "NEXT_PUBLIC_SUPABASE_URL": { "fromAny": ["NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_URL"], "required": true },
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": { "fromAny": ["NEXT_PUBLIC_SUPABASE_ANON_KEY", "SUPABASE_ANON_KEY"], "required": true },
        "SUPABASE_SERVICE_ROLE_KEY": { "fromAny": ["SUPABASE_SERVICE_ROLE_KEY", "SUPABASE_SERVICE_KEY"], "required": false },
        "STRIPE_SECRET_KEY": { "from": "STRIPE_SECRET_KEY", "required": false },
        "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY": { "from": "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY", "required": false },
        "GOOGLE_CUSTOM_SEARCH_API_KEY": { "from": "GOOGLE_CUSTOM_SEARCH_API_KEY", "required": false },
        "GOOGLE_CUSTOM_SEARCH_ENGINE_ID": { "from": "GOOGLE_CUSTOM_SEARCH_ENGINE_ID", "required": false },
        "KILO_API_KEY": { "from": "KILO_API_KEY", "required": false },
        "ADMIN_PASSWORD": { "from": "ADMIN_PASSWORD", "required": false }
      }
    }
  ]
}
