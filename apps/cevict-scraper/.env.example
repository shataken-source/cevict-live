# CevictScraper Environment Configuration

# Server Settings
PORT=3009
NODE_ENV=development

# CORS - Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3008,http://localhost:3010

# Browser Settings
HEADLESS=true
MAX_BROWSERS=3
MAX_PAGES_PER_BROWSER=5
BROWSER_TIMEOUT=30000

# Rate Limiting (ms between requests to same domain)
RATE_LIMIT_PER_DOMAIN=1000

# Cache Settings
CACHE_ENABLED=false
CACHE_TTL=3600000

# Proxy Settings (optional)
# PROXY_URL=http://proxy.example.com:8080

# Logging
LOG_LEVEL=info
