{
  "expo": {
    "name": "IPTV Viewer",
    "slug": "iptvviewer",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cevict.iptvviewer"
    },
    "android": {
      "package": "com.cevict.iptvviewer",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow IPTV Viewer to access your microphone."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9b026c62-c143-4013-9f23-e8a5b920d520"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    }
  }
}
