{
  "app": "auspicio",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "PORT": { "from": "PORT", "required": false },
        "OPENAI_API_KEY": { "from": "OPENAI_API_KEY", "required": false },
        "ANTHROPIC_API_KEY": { "from": "ANTHROPIC_API_KEY", "required": false },
        "GOOGLE_API_KEY": { "fromAny": ["GOOGLE_API_KEY", "GEMINI_API_KEY"], "required": false },
        "VERCEL_OIDC_TOKEN": { "from": "VERCEL_OIDC_TOKEN", "required": false }
      }
    }
  ]
}
