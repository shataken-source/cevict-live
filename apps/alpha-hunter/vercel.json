{
  "framework": null,
  "buildCommand": "tsc",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "tsx src/index.ts",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
