{
  "name": "sportsbook-terminal",
  "description": "Sportsbook trading terminal for prognostication",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "SUPABASE_URL": { "from": "SUPABASE_URL", "required": true },
        "SUPABASE_SERVICE_KEY": { "from": "SUPABASE_SERVICE_ROLE_KEY", "required": true },
        "FRONTEND_URL": { "literal": "http://localhost:3000", "required": false },
        "PROGNO_URL": { "from": "PROGNO_URL", "required": false },
        "PROGNO_API_KEY": { "from": "PROGNO_INTERNAL_API_KEY", "required": false },
        "CORS_ORIGINS": { "from": "CORS_ORIGINS", "required": false }
      }
    }
  ]
}
