{
  "name": "sportsbook-terminal",
  "description": "Sportsbook trading terminal for prognostication",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "SUPABASE_URL": {
          "from": "SUPABASE_URL",
          "required": true
        },
        "SUPABASE_SERVICE_KEY": {
          "from": "SUPABASE_SERVICE_ROLE_KEY",
          "required": true
        },
        "FRONTEND_URL": {
          "literal": "http://localhost:3000",
          "required": false
        }
      }
    }
  ]
}
