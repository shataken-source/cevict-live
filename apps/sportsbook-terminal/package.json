{
  "name": "sportsbook-terminal",
  "version": "1.0.0",
  "description": "Institutional Probability Trading Terminal - Prognostication Capital",
  "main": "api/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn api/server.ts",
    "build": "tsc",
    "start": "node dist/api/server.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:migrate": "psql -d $DATABASE_URL -f database/schema.sql",
    "db:seed": "ts-node scripts/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "@supabase/supabase-js": "^2.39.0",
    "stripe": "^14.9.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "sportsbook",
    "kalshi",
    "polymarket",
    "probability",
    "kelly-criterion",
    "monte-carlo",
    "trading",
    "hedge-fund"
  ],
  "author": "Prognostication Capital",
  "license": "MIT"
}
