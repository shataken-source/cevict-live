{
  "app": "alpha-hunter",
  "outputs": [
    {
      "file": ".env.local",
      "vars": {
        "ANTHROPIC_API_KEY": {
          "from": "ANTHROPIC_API_KEY",
          "required": true
        },
        "NEXT_PUBLIC_SUPABASE_URL": {
          "fromAny": ["NEXT_PUBLIC_SUPABASE_URL", "SUPABASE_URL"],
          "required": true
        },
        "SUPABASE_SERVICE_ROLE_KEY": {
          "fromAny": ["SUPABASE_SERVICE_ROLE_KEY", "SUPABASE_SERVICE_KEY"],
          "required": true
        },
        "KALSHI_API_KEY_ID": {
          "from": "KALSHI_API_KEY_ID",
          "required": true
        },
        "KALSHI_PRIVATE_KEY": {
          "from": "KALSHI_PRIVATE_KEY",
          "required": true
        },
        "KALSHI_ENV": {
          "from": "KALSHI_ENV",
          "required": false,
          "default": "production"
        },
        "COINBASE_API_KEY": {
          "from": "COINBASE_API_KEY",
          "required": true
        },
        "COINBASE_API_SECRET": {
          "from": "COINBASE_API_SECRET",
          "required": true
        },
        "PROGNO_BASE_URL": {
          "from": "PROGNO_BASE_URL",
          "required": false,
          "default": "https://progno.vercel.app"
        },
        "OPENWEATHER_API_KEY": {
          "from": "OPENWEATHER_API_KEY",
          "required": false
        },
        "ODDS_API_KEY": {
          "from": "ODDS_API_KEY",
          "required": false
        }
      }
    }
  ]
}
